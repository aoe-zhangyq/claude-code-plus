<idea-plugin>
    <!--
    Subversion (SVN) 可选依赖配置

    当 Subversion 插件安装时，此文件会被加载。
    SvnServiceImpl 会覆盖默认的 NoopSvnService，
    提供完整的 SVN 版本控制操作功能。
    -->
    <extensions defaultExtensionNs="com.intellij">
        <!-- 覆盖默认的 NoopSvnService -->
        <projectService
            serviceInterface="com.asakii.plugin.services.SvnService"
            serviceImplementation="com.asakii.plugin.services.SvnServiceImpl"
            overrides="true"/>
    </extensions>
</idea-plugin>
